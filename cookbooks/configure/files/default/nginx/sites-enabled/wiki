server {
	listen 8081;

	index index.php index.html;
	root /vagrant/wiki/www;

	location / {
		try_files $uri $uri/ /index.php;
	}

	location ~* \.php$ {
		include /etc/nginx/fastcgi_params;
		keepalive_timeout 0;

		fastcgi_param SCRIPT_FILENAME $document_root$fastcgi_script_name;
		fastcgi_pass 127.0.0.1:9000;
	}
}
